<div class="modal-dialog modal-lg" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <h5 class="modal-title">{{ 'vs_cms.template.filemanager_update_upload_file' | trans( {},'VSCmsBundle' ) }}</h5>
            <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                <span aria-hidden="true">&times;</span>
            </button>
        </div>
        <div class="modal-body">
            {{ form_start(formUploadFile, { 'method': 'POST', 'attr': {'class': 'form-horizontal', 'id': 'formUploadFile'} }) }}
                {{ form_widget(formUploadFile._token) }}   
            
                <div class="form-group">
                    {{ form_label(formUploadFile.newTitle, null, {
                        'label_attr': {'class': 'col-form-label'}
                        }) 
                    }}
                    {{ form_widget(formUploadFile.newTitle, {
                        'attr': {
                            'class': 'form-control',
                        }
                    }) }}
                </div>
            {{ form_end(formClone, {'render_rest': false}) }}
        </div>
        <div class="modal-footer" id="imContainer">
            <button type="button" class="btn btn-primary" id="btnSaveFormClone">
                {{ 'vs_cms.template.filemanager_update_button_upload' | trans( {},'VSCmsBundle' ) }}
            </button>
            <button type="button" class="btn btn-secondary" data-dismiss="modal">
                {{ 'vs_application.form.cancel' | trans( {},'VSApplicationBundle' ) }}
            </button>
        </div>
    </div>
</div>
