<div id="multipageTocPageModal" class="modal" tabindex="-1" role="dialog" aria-hidden="true">
    <div class="modal-dialog modal-lg" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <h5 class="modal-title">
					{{ 'vs_cms.modal.multipage_toc_page.title' | trans( {},'VSCmsBundle' ) }}
				</h5>
                <button type="button" class="close" data-dismiss="modal" aria-label="Close">
                    <span aria-hidden="true">&times;</span>
                </button>
            </div>
            <div class="modal-body">
                <div id="modalBodyTocPage" class="card alert shadow mb-2">
                    <div class="card-body"></div>
                </div>
            </div>
            <div class="modal-footer" id="imContainer">
                <button type="button" class="btn btn-primary" id="btnSaveTocPage">
					{{ 'vs_application.form.save' | trans( {},'VSApplicationBundle' ) }}
				</button>
				<button type="button" class="btn btn-primary" style="display: none;">
					{{ 'vs_application.form.apply' | trans( {},'VSApplicationBundle' ) }}
				</button>
                <button type="button" class="btn btn-secondary" data-dismiss="modal">
					{{ 'vs_application.form.cancel' | trans( {},'VSApplicationBundle' ) }}
				</button>
            </div>
        </div>
    </div>
</div>
