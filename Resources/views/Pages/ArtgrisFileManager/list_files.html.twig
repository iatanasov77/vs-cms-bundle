{% extends "@VSApplication/layout.html.twig" %}

{% block title %}{{ parent() }} :: {{ 'vs_cms.template.filemanager_index_title' | trans( {},'VSCmsBundle' ) }}{% endblock %}
{% block pageTitle %}<i class="icon_genius"></i> {{ 'vs_cms.template.filemanager_index_title' | trans( {},'VSCmsBundle' ) }}{% endblock %}

{% block content %}
	<div class="container-fluid  dashboard-content">
        <!-- ============================================================== -->
        <!-- pageheader -->
        <!-- ============================================================== -->
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
                <div class="page-header">
                    <h2 class="pageheader-title">File Manager</h2>
                    
                    <p class="pageheader-text">Proin placerat ante duiullam scelerisque a velit ac porta, fusce sit amet vestibulum mi. Morbi lobortis pulvinar quam.</p>
                    <div class="page-breadcrumb">
                        <nav aria-label="breadcrumb">
                            {{ knp_menu_render( 'breadcrumbs', {'template': '@VSApplication/Menu/breadcrumb.html.twig'} ) }}
                        </nav>
                    </div>
                </div>
            </div>
        </div>
        <!-- ============================================================== -->
        <!-- end pageheader -->
        <!-- ============================================================== -->
        
        <div class="row">
            <div class="col-xl-12 col-lg-12 col-md-12 col-sm-12 col-12">
            
            	<iframe src="{{ path( 'file_manager' ) }}?conf=default" style="width: 100%; height: 400px;"></iframe>
            	
            </div>
        </div>
    </div>
{% endblock %}
