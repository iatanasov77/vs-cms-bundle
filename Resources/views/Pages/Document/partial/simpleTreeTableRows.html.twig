<tr data-node-id="{{ tocPage.id }}" {% if parentId %}data-node-pid="{{ tocPage.parent.id }}"{% endif %}>
    <td>{{ tocPage.name }}</td>
    <td>
    	<a href="javascript:void(0);"
    	   data-tocPageId="{{ tocPage.id }}"
    	>
    		{{ 'vs_cms.template.page_categories_index_button_edit' | trans( {},'VSCmsBundle' ) }}
    	</a>
    </td>
</tr>

{% for child in tocPage.children %}
	{% include '@VSCms/Pages/Page_Categories/partial/simpleTreeTableRows.html.twig' with {'parentId': tocPage.id, 'tocPage': child} %}   
{% endfor %}
