{
	"name": "vankosoft/cms-bundle",
	"type": "symfony-bundle",
	"description": "Symfony CMS Bundle",
	"keywords": ["CMS", "HTML", "Editor", "Image Manipulation"],
	"homepage": "https://github.com/iatanasov77/vs-cms-bundle",
	"license": "MIT",
	"authors": [
		{
			"name": "Ivan I. Atanasov",
			"email": "i.atanasov77@gmail.com"
		}
	],
	"require": {
		"php": "^7.1|^8.0",
		
		"vankosoft/application-bundle": "^1.4",
		
		"friendsofsymfony/ckeditor-bundle": "^2.0",
        "liip/imagine-bundle": "^2.6",
        "artgris/filemanager-bundle": "^2.2",
        "knplabs/knp-gaufrette-bundle": "^0.7",
        "spe/filesize-extension-bundle": "^2.0"
	},
	"conflict": {
		"doctrine/doctrine-bundle" : "<1.3",
		"symfony/doctrine-bridge" : "<2.7"
	},
	"autoload": {
		"psr-4": {
			"Vankosoft\\CmsBundle\\": "lib/"
		}
	},
    "repositories": [
        {
            "type": "path",
            "url": "../*"
        }
    ],
	"minimum-stability": "dev"
}
